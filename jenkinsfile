pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Check out the code from the Git repository
                git 'https://github.com/Farooq722/GolfClub.git'
            }
        }

        stage('Build') {
            steps {
                // Replace this with your build commands
                sh 'echo "Building the project..."'
            }
        }
    }

    post {
        failure {
            echo "Pipeline failed! Notify the team or take appropriate actions."
        }
    }
}
